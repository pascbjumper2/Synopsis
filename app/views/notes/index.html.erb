<%= link_to "New Note", new_note_path %>
<% @notes.each do |note| %>
  <h3><%= note.title.html_safe %></h3>
  <p><%= note.body.html_safe %></p>
  <%= link_to "Edit Note", edit_note_path(note) %>
  <% if note != @notes.last %>
  <hr />
  <% end %>
<% end %>


<br /><br />
<% if user_signed_in? %><%= current_user.name.capitalize %><% end %>

